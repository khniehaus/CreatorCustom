QMenu::separator
{
    height: 2px;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ffa0ce, stop: 0.5 #ffa0ce, stop: 0.6 #ffa0ce, stop:1 #ffa0ce);
    color: #510028;
    padding-left: 4px;
    margin-left: 10px;
    margin-right: 5px;
}

QProgressBar
{
    border: 2px solid #ffa0ce;;
    border-radius: 5px;
    text-align: center;
}

QProgressBar::chunk
{
    background-color: #a3537b;
    width: 2.15px;
    margin: 0.5px;
}

QTabBar::tab {
    /*color: #b1b1b1;*/
    color: #f5207c; /*hot pink*/
    border: 1px solid #510028;
    /*light pink f27bb6*/
    border-bottom-style: none;
    background-color: #a3537b; /*dark pink*/
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 3px;
    padding-bottom: 2px;
    margin-bottom: -2px;
}

QTabWidget::pane {
    border: 1px solid #510028;
    top: 1px;
}

QTabBar::tab:last
{
    margin-right: 0; /* the last selected tab has nothing to overlap with on the right */
    border-top-right-radius: 3px;
}

QTabBar::tab:first:!selected
{
 margin-left: 0px; /* the last selected tab has nothing to overlap with on the right */


    border-top-left-radius: 3px;
}

QTabBar::tab:!selected
{
    border-bottom-style: solid;
    margin-top: 3px;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:1 #212121, stop:.4 #700138);
}

QTabBar::tab:selected
{
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    margin-bottom: 0px;
    border: 1px solid #f27bb6;
    border-bottom: 0px;
    /*background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #8f8f8f, stop:1 #454545);*/
    /*background-color: #d7801a;*/
    /*background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:1 #d7801a, stop:.4 #ffaa00);*/
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f27bb6, stop:1 #ff6db6);
}

QTabBar::tab:!selected:hover
{
    /*border-top: 2px solid #ffaa00;
    padding-bottom: 3px;*/
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:1 #f27bb6, stop:0.4 #ff87c1, stop:0.2 #ff87c1, stop:0.1 #ff5bad);
}

QRadioButton::indicator:checked, QRadioButton::indicator:unchecked{
    color: #510028;
    background-color: #510028;
    border: 1px solid #f27bb6;
    border-radius: 6px;
}

QRadioButton::indicator:checked, QListView::indicator:checked
{
    background-color: qradialgradient(
        cx: 0.5, cy: 0.5,
        fx: 0.5, fy: 0.5,
        radius: 1.0,
        stop: 0.25 #f5207c,
        stop: 0.3 #510028
    );
}

QCheckBox::indicator, QListView::indicator{
    color: #f5207c;
    background-color: #510028;
    border: 1px solid #f5207c;
    width: 9px;
    height: 9px;
}

QRadioButton::indicator
{
    border-radius: 6px;
}

QRadioButton::indicator:hover, QCheckBox::indicator:hover, QListView::indicator:hover
{
    border: 1px solid #ffa0ce;
}

QCheckBox::indicator:checked
{
    image:url(data/themes/makehuman/images/checkbox-orange.png);
}

QCheckBox::indicator:disabled, QRadioButton::indicator:disabled, QListView::indicator:disabled
{
    border: 1px solid #510028;
}

FileChooser > QWidget {
    padding: -2px -2px;
}

FileChooserRectangle {
    margin: 2px 2px;
}

ListFileChooser ListView {
    background-color: #510028;
}

QListView::indicator
{
    border-radius: 8px;
    width: 15px;
    height: 15px;
}

QTreeView::branch {
    background-color: #510028;
}

QTreeView::branch:open:has-children {
    image: url(data/themes/makehuman/images/branch-open.png);
}

QTreeView::branch:closed:has-children {
    image: url(data/themes/makehuman/images/branch-closed.png);
}

